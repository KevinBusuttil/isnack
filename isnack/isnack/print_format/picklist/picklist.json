 "font_size": 14,
 "html": "<style>\n  .picklist-print {\n    font-size: 10pt;\n    font-family: sans-serif;\n  }\n  .picklist-print h2 {\n    margin: 0 0 4px 0;\n    font-size: 14pt;\n  }\n  .picklist-meta {\n    margin-bottom: 8px;\n    width: 100%;\n  }\n  .picklist-meta td {\n    padding: 2px 4px;\n    vertical-align: top;\n  }\n  .picklist-section-title {\n    margin-top: 12px;\n    font-weight: bold;\n    text-transform: uppercase;\n    font-size: 10pt;\n    border-bottom: 1px solid #eee;\n    padding-bottom: 2px;\n  }\n  table.picklist-table {\n    width: 100%;\n    border-collapse: collapse;\n    margin-top: 4px;\n  }\n  table.picklist-table th,\n  table.picklist-table td {\n    border: 1px solid #000;\n    padding: 3px 4px;\n  }\n  table.picklist-table th {\n    text-align: left;\n    font-size: 10pt;\n    background-color: #f8f8f8;\n  }\n  .text-right {\n    text-align: right;\n  }\n  .muted {\n    color: #666;\n    font-size: 8pt;\n  }\n  .signature-row td {\n    padding-top: 30px;\n  }\n  \n  @media screen {\n    .print-format {\n    \twidth: 200mm;\n    \theight: 270mm;\n    \tpadding: 3mm;\n    }\n    \t\n    /* This makes the font smaller only on your monitor */\n    .picklist-print {\n      font-size: 8pt !important; \n    }\n    \n    /* Scaling down headers specifically for screen */\n    .picklist-print h2 {\n      font-size: 12pt !important;\n    }\n\n    .picklist-section-title {\n      font-size: 8pt !important;\n    }\n\n    table.picklist-table th, \n    table.picklist-table td {\n      font-size: 7pt !important;\n    }    \n    .picklist-meta td {\n    font-size: 8pt !important;\n    }\n  }  \n</style>\n\n<div class=\"picklist-print\">\n\n  {# --- Header --- #}\n  <h2>Picklist</h2>\n\n  <table class=\"picklist-meta\">\n    <tr>\n      <td style=\"width: 15%;\"><b>Picklist No:</b></td>\n      <td style=\"width: 35%;\">{{ doc.name }}</td>\n      <td style=\"width: 15%;\"><b>Date:</b></td>\n      <td style=\"width: 35%;\">{{ frappe.utils.formatdate(doc.posting_date or doc.creation) }}</td>\n    </tr>\n    <tr>\n      <td><b>Company:</b></td>\n      <td>{{ doc.company }}</td>\n      <td><b>Printed On:</b></td>\n      <td>{{ frappe.utils.format_datetime(frappe.utils.now()) }}</td>\n    </tr>\n    <tr>\n      <td><b>From Warehouse:</b></td>\n      <td>{{ doc.from_warehouse or '' }}</td>\n      <td><b>To Warehouse:</b></td>\n      <td>{{ doc.to_warehouse or '' }}</td>\n    </tr>\n  </table>\n\n  {# --- Transfers included --- #}\n  <div class=\"picklist-section-title\">Transfers / Stock Entries</div>\n  {% if doc.transfers %}\n    <table class=\"picklist-table\">\n      <thead>\n        <tr>\n          <th style=\"width: 10%;\">#</th>\n          <th>Stock Entry</th>\n        </tr>\n      </thead>\n      <tbody>\n        {% for t in doc.transfers %}\n        <tr>\n          <td>{{ loop.index }}</td>\n          <td>{{ t.stock_entry }}</td>\n        </tr>\n        {% endfor %}\n      </tbody>\n    </table>\n  {% else %}\n    <div class=\"muted\">No transfers linked to this picklist.</div>\n  {% endif %}\n\n  {# --- Work Orders Section --- #}\n  {% if doc.transfers %}\n    {% set stock_entry_list = [] %}\n    {% for t in doc.transfers %}\n        {% if t.stock_entry %}\n            {% set _ = stock_entry_list.append(t.stock_entry) %}\n        {% endif %}\n    {% endfor %}\n    \n    {% if stock_entry_list %}\n      {% set work_orders = frappe.db.sql(\"\"\"\n        SELECT DISTINCT\n          wo.name as work_order,\n          wo.production_item as item_code,\n          wo.item_name\n        FROM `tabWork Order` wo\n        INNER JOIN `tabStock Entry` se ON se.work_order = wo.name\n        WHERE se.name IN %(stock_entries)s\n          AND se.docstatus = 1\n        ORDER BY wo.name\n      \"\"\", {\"stock_entries\": stock_entry_list}, as_dict=True) %}\n      \n      {% if work_orders %}\n        <div class=\"picklist-section-title\">Work Orders</div>\n        <table class=\"picklist-table\">\n          <thead>\n            <tr>\n              <th style=\"width: 10%;\">#</th>\n              <th style=\"width: 25%;\">Work Order</th>\n              <th style=\"width: 25%;\">Produced Item Code</th>\n              <th style=\"width: 40%;\">Produced Item Name</th>\n            </tr>\n          </thead>\n          <tbody>\n            {% for wo in work_orders %}\n            <tr>\n              <td>{{ loop.index }}</td>\n              <td>{{ wo.work_order }}</td>\n              <td>{{ wo.item_code }}</td>\n              <td>{{ wo.item_name or '' }}</td>\n            </tr>\n            {% endfor %}\n          </tbody>\n        </table>\n      {% endif %}\n    {% endif %}\n  {% endif %}\n\n  {# --- Items to pick --- #}\n  <div class=\"picklist-section-title\">Items to Pick</div>\n\n  {% set items = doc.items | sort(attribute='from_warehouse') %}\n\n  {% if items %}\n    <table class=\"picklist-table\">\n      <thead>\n        <tr>\n          <th style=\"width: 3%;\">#</th>\n          <th style=\"width: 15%;\">Item Code</th>\n          <th>Item Name</th>\n          <th style=\"width: 10%;\">Batch</th>\n          <th style=\"width: 12%;\">From Whse</th>\n          <th style=\"width: 12%;\">To Whse</th>\n          <th style=\"width: 8%;\">Qty</th>\n          <th style=\"width: 7%;\">UOM</th>\n          <th style=\"width: 9%;\">Picked</th>\n        </tr>\n      </thead>\n      <tbody>\n        {% for row in items %}\n        <tr>\n          <td>{{ loop.index }}</td>\n          <td>{{ row.item_code }}</td>\n          <td>{{ row.item_name or \"\" }}</td>\n          <td>{{ row.batch_no or \"\" }}</td>\n          <td>{{ row.from_warehouse or doc.from_warehouse or \"\" }}</td>\n          <td>{{ row.to_warehouse or doc.to_warehouse or \"\" }}</td>\n          <td class=\"text-right\">\n            {% if row.qty %}\n              {{ \"%.2f\"|format(row.qty) }}\n            {% endif %}\n          </td>\n          <td>{{ row.uom or \"\" }}</td>\n          <td></td>\n        </tr>\n        {% endfor %}\n      </tbody>\n    </table>\n  {% else %}\n    <div class=\"muted\">No items on this picklist.</div>\n  {% endif %}\n\n  {# --- Signatures --- #}\n  <table style=\"width: 100%; margin-top: 30px;\">\n    <tr class=\"signature-row\">\n      <td style=\"width: 33%; border: none;\">Picker: ________________</td>\n      <td style=\"width: 33%; border: none;\">Checked By: ____________</td>\n      <td style=\"width: 34%; border: none;\">Date: __________________</td>\n    </tr>\n  </table>\n\n</div>",
